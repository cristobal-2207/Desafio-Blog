<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Date</th>
      <th>Image</th>
      <th>Content</th>
    </tr>
  </thead>
  <tbody>
		<% @posts.each do |post| %>
			<tr>
				<td><%= post.title %></td>
        <td><%= post.date %></td>
        <td><%= post.image_url %></td>
        <td><%= post.content %></td>
			<tr>
		<% end %>
  </tbody>
</table>
